        "js-file-download": "^0.4.12",
        "lint": "^0.7.0",
        "moment": "^2.29.1",
        "onesignal-node": "^3.2.1",
        "roboto-fontface": "^0.10.0",
        "sweetalert2": "^11.3.4",
        "v-select2-component": "^0.4.7",
        "v-tooltip": "^2.1.3",
        "vee-validate": "^3.4.14",
